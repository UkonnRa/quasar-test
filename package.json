{
  "name": "quasar-test",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar Test",
  "author": "Ukonn Ra <ukonnra@outlook.com>",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20",
    "yarn": ">= 4.2.2"
  },
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "postinstall": "husky",
    "lint": "yarn lint:tsc && yarn lint:prettier --check && yarn lint:eslint",
    "lint:tsc": "vue-tsc --noEmit -p tsconfig.vue-tsc.json",
    "lint:eslint": "eslint '**/*.{vue,ts,mts,js}'",
    "lint:prettier": "prettier '**/*.{vue,ts,mts,js,json}'",
    "lint:fix": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:all": "quasar build -m ssr && quasar build -m pwa && quasar build -m electron && quasar build",
    "serve": "node './node_modules/@quasar/cli/bin/quasar.js' serve",
    "upgrade": "yarn upgrade-interactive",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.11",
    "pinia": "^2.1.7",
    "quasar": "^2.16.4",
    "register-service-worker": "^1.7.2",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@electron/packager": "^18.3.2",
    "@eslint/js": "^9.4.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.0.0-beta.14",
    "@quasar/cli": "^2.4.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/node": "^20.14.2",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "dotenv": "^16.4.5",
    "electron": "^30.1.0",
    "eslint": "~9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.1",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.12.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19",
    "workbox-build": "^7.1.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
